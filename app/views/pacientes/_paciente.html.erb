<div id="<%= dom_id paciente %>">
  <p>
    <strong>Nome:</strong>
    <%= paciente.nome %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= paciente.cpf %>
  </p>

  <p>
    <strong>Sexo:</strong>
    <%= paciente.sexo %>
  </p>

  <p>
    <strong>Data de Nascimento:</strong>
    <%= paciente.data_nascimento&.strftime("%d/%m/%Y") %>
  </p>

  <p>
    <strong>Celular:</strong>
    <%= paciente.celular %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= paciente.email %>
  </p>

  <p>
    <strong>Nome Social:</strong>
    <%= paciente.nome_social %>
  </p>

  <p>
    <strong>Horário:</strong>
    <%= paciente.horario %>
  </p>

  <p>
    <strong>Observação:</strong>
    <%= paciente.observacao %>
  </p>

</div>
